@model List<Booking>

<h4 class="text-center">Функция агрегации Match и Project по коллекции "booking"</h4>
<table class="account_booking">
<tr class="account_booking_item">
    <td><span>Идентификатор</span></td>
    <td><span>Название зала</span></td>
    <td><span>Дата создания</span></td>
    <td><span>Старт аренды</span></td>
    <td><span>Конец аренды</span></td>
    <td><span>Статус</span></td>
</tr>
@foreach (var booking in Model)
{
    <tr class="account_booking_item">
        <td><span>@booking.IdBooking</span></td>
        <td><span>@booking.RentedHall.Name</span></td>
        <td><span>@booking.CreatingDateTime.ToString("g")</span></td>
        <td><span>@booking.StartHallReserving.ToString("g")</span></td>
        <td><span>@booking.EndHallReserving.ToString("g")</span></td>
        <td><span>@booking.Status</span></td>
    </tr>
}
</table>